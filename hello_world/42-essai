#include <stdio.h>

	int main(void) {
    int a = 5, b = 10, sum;
if (a > b) {
        sum = a + b;
        printf("Sum is: %d\n", sum);
    } else 
{
        printf("a is not greater than b\n");
    }

    for (int i = 0; i < 5; i++) {
        printf("%d ", i);
    }
    printf("\n");

    return 0;
}
